checkquest()

local DistanceQ = (CFrameQ.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude

function buso() --- use buso haki
    if(not(game.Players.LocalPlayer.Character:FindFirstChild('HasBuso')))then
        game.ReplicatedStorage.Remotes.CommF_:InvokeServer('Buso')
    end
end

function equip(typ) --- equip tool
    for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if(v:IsA('Tool'))then
            if(v.Name==typ)then
                game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
            end
        end
    end
end

function to(a) --- Tween
    part = Instance.new("Part",workspace)
    part.CanCollide = false
    part.Anchored = true
    part.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    part.Transparency = 1
    tweenser, info=game:GetService('TweenService'), TweenInfo.new((a.Position-part.Position).Magnitude/300, Enum.EasingStyle.Linear)
    if tween == nil then
        tween = tweenser:Create(part, info, {CFrame = a})
    end
    tween:Play()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=part.CFrame
    tween.Completed:Connect(function()
        part:Destroy()
    end)
    if (a.Position-part.Position).Magnitude<=275 then
        part.CFrame=a
    end
    if game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame ~= a then
    if game.workspace:FindFirstChild("Part") then
        while task.wait(0.01) do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part.CFrame
        if game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame == a then
            break
        end
        if not game.workspace:FindFirstChild("Part") then
            tween:Cancel()
            break
        end
            end
        end
        part.Name = "Partx2"
    else
        print("not found")
    end
    
    spawn(function ()
        if game.workspace:FindFirstChild('Partx2') then
            part:Destroy()
        end
    end)

    function stopTween()
        if game.workspace:FindFirstChild("Part") then
            game.workspace:FindFirstChild("Part"):Destroy()
            tween:Cancel()
        end
    end
end

function TweenY(gotoCFrame) --- Tween
    pcall(function()
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
    end)
    if (game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.Position - gotoCFrame.Position).Magnitude <= 150 then
        pcall(function() 
            tween:Cancel()
        end)
        game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.CFrame = gotoCFrame
    else
        local tween_s = game:service"TweenService"
        local info = TweenInfo.new((game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.Position - gotoCFrame.Position).Magnitude/300, Enum.EasingStyle.Linear)
        local tween, err = pcall(function()
            tween = tween_s:Create(game.Players.LocalPlayer.Character["HumanoidRootPart"], info, {CFrame = gotoCFrame})
            tween:Play()
        end)
        if not tween then return err end
    end
end

function two(target) --- Tween
    tws, info=game:GetService('TweenService'), TweenInfo.new(distance(target.Position)/325, Enum.EasingStyle.Linear)
    tw=tws:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, info, {CFrame=target})
        pcall(function()
            tw:Play()
            if(distance(target.Position)<=250)then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=target
            end
        end)
        function stop()
            pcall(function()
                tw:Cancel()
            end)
        end
end

function distance(targetpos) --- distance form Position
    return(game.Players.LocalPlayer:DistanceFromCharacter(targetpos))
end

function tp(target) --- teleport
    Distance = (target.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    print(Distance)
    if Distance < 1000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target
    end
end

function noclip(a, a2) --- noclip
    game.Players.PlayerAdded:Connect(function()
        if not a:FindFirstChild('BodyVelocity') then
            local ve = Instance.new('BodyVelocity', a)
            ve.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            ve.Velocity = Vector3.new(0, 0, 0)
        end    
    end)
    for i, v in pairs(a2:GetDescendants()) do
        if v.Name ~= 'RightFoot' or v.Name ~= 'LeftFoot' then
            pcall(function()
                v.CanCollide = false
                if not nocliping then
                    v.Touched:Connect(function(a)
                        a.CanCollide = false
                        task.wait(0.5)
                        a.CanCollide = true
                    end)
                    nocliping = true
                end
            end)
        end
    end
end

function doingquest() --- checkquest
    return(game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible)
end

spawn(function()
    while wait() do 
        pcall(function()
            local mob = game.Workspace.Enemies:FindFirstChild(Ms)
            mobcframe = mob.HumanoidRootPart.CFrame
            mobname = mob.Name
            mob.HumanoidRootPart.CanCollide = false
            mob.Head.CanCollide = false
            mob.Humanoid.WalkSpeed=0
        end)
    end
end)

spawn(function()
    game:GetService('RunService').Heartbeat:Connect(function()
        pcall(function()
            for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
                if v:FindFirstChild('Humanoid') and v:FindFirstChild('HumanoidRootPart') then
                    local dis = (v.HumanoidRootPart.Position -  game.Players.localPlayer.Character.HumanoidRootPart.Position).Magnitude
                        if v.Name == mobname then
                        if dis <= 300 then
                            v.Head.CanCollide = false
                            v.HumanoidRootPart.CanCollide = false
                            v.HumanoidRootPart.CFrame = mobcframe
                            v.Humanoid.WalkSpeed=0
                            sethiddenproperty(game.Players.LocalPlayer, 'SimulationRadius', math.huge) 
                        end
                    end
                end
            end
        end)
    end)
end)

function autofarm()
    if getgenv().Config.autofarm then
        while task.wait() do
            if (doingquest()==false) then
            checkquest()
            noclip(game.Players.localPlayer.Character, game.Players.localPlayer.Character)
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
            if DistanceQ < 1000 then
                two(CFrameQ)
                equip(getgenv().Config.weapon)
                noclip(game.Players.localPlayer.Character.HumanoidRootPart, game.Players.localPlayer.Character)
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
            elseif DistanceQ > 1000 then
                two(CFrameQ)
                noclip(game.Players.localPlayer.Character.HumanoidRootPart, game.Players.localPlayer.Character)
                equip(getgenv().Config.weapon)
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
            end
            local dq = (CFrameQ.Position - game.Players.localPlayer.Character.HumanoidRootPart.Position).Magnitude 
            if dq <= 5 then
                game.ReplicatedStorage.Remotes.CommF_:InvokeServer('StartQuest', NameQuest, QuestLv)
                noclip(game.Players.localPlayer.Character, game.Players.localPlayer.Character)
            end
        elseif (doingquest()==true) then
            checkquest()
            equip(getgenv().Config.weapon)
            if game:GetService("Workspace").Enemies:FindFirstChild(Ms) then
                for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    if v.Name == Ms and v:FindFirstChild("Humanoid") then
                        if v.Humanoid.Health > 0 then
                            repeat wait()
                                if game:GetService("Workspace").Enemies:FindFirstChild(Ms) then
                                    noclip(game.Players.localPlayer.Character, game.Players.localPlayer.Character)
                                    equip(getgenv().Config.weapon)
                                    if string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, NameMon) then
                                        two(v.HumanoidRootPart.CFrame * CFrame.new(0 , 25 , 5))
                                    else
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                                    end
                                else
                                    if (CFrameMon.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude >= 600 then
                                        checkquest()
                                        equip(getgenv().Config.weapon)
                                        two(CFrameMon)
                                    end
                                end
                            until not v.Parent or v.Humanoid.Health <= 0 or getgenv().Config.autofarm == false or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false or not game:GetService("Workspace").Enemies:FindFirstChild(Ms)
                        end
                    end
                end
            else
            if (CFrameMon.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude >= 600  then
                checkquest()
                two(CFrameMon)
                    end 
                end
            end
        if getgenv().Config.autofarm == false then
            break
        end
        end      
    end
end

spawn(function()
    if getgenv().Config.autofarm then
        autofarm() 
    elseif getgenv().Config.autofarm then
        spawn(function()
            while task.wait() do
                for i,v in pairs(game.Players.localPlayer.Character.HumanoidRootPart:GetChildren()) do
                    if v.Naem == "BodyVelocity" then
                        v:Destroy()
                    end
                end
                if getgenv().Config.autofarm == false then
                    break
                end
            end
        end)
    end
end)

spawn(function()
    while task.wait() do
        if getgenv().Config.autofarm then
            autofarm() 
        elseif getgenv().Config.autofarm then
            game:GetService("RunService").Stepped:Connect(function()
            if getgenv().Config.noclip == false then
                for i, v in pairs(game.Players.localPlayer.Character:GetDescendants()) do
                    if v:IsA('BasePart') then
                        v.CanCollide = true
                        end    
                    end
                end
            end)
        end
    end
end)

spawn(function()
    while task.wait() do
        buso()
    end
end)


spawn(function() --- Delete Effect
    game:GetService("RunService").Stepped:Connect(function()
        for i,v in pairs(game:GetService("Workspace")["_WorldOrigin"]:GetChildren()) do
            pcall(function()
                if v.Name == ("CurvedRing") or v.Name == ("SlashHit") or v.Name == ("DamageCounter") or v.Name == ("SwordSlash") or v.Name == ("SlashTail") or v.Name == ("Sounds") then
                    v:Destroy()
                end
            end)
        end
    end)
end)

spawn(function() --- Fast Attack
	while getgenv().Config.fastattack do
        pcall(function()
            local cam = require(game.Players.LocalPlayer.PlayerScripts.CombatFramework.CameraShaker)
            local VirtualUser = game:GetService('VirtualUser')
            cam.CameraShakeInstance.CameraShakeState.Inactive = 0
            game:GetService('VirtualUser'):CaptureController()
            game:GetService('VirtualUser'):Button1Down(Vector2.new(1280, 672))
            task.wait(0.1)
        end) 
    end
end)

spawn(function()----hitbox
while task.wait() do
    for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
        if v:FindFirstChild('Humanoid') then
            v.HumanoidRootPart.Transparency = 1
            v.HumanoidRootPart.Size = Vector3.new(50,50,50)
            end
        end
    end
end)

spawn(function()
    while task.wait() do  
     for i,v in pairs(game.CoreGui:GetChildren()) do
            if v:FindFirstChild('Selection') then
       game.Players.LocalPlayer:Kick('nono ห้ามเปิด darkdex')
            end
        end
    end
end)

spawn(function()------ใต้เท้า
	game:GetService("RunService").Stepped:Connect(function()
		if getgenv().Config.autofarm then
			if not game:GetService("Workspace"):FindFirstChild("LOL") then
				local LOL = Instance.new("Part")
				LOL.Name = "LOL"
				LOL.Parent = game.Workspace
				LOL.Anchored = true
				LOL.Transparency = 1
				LOL.Size = Vector3.new(20,-0.5,20)
				LOL.Material = "Neon"
			elseif game:GetService("Workspace"):FindFirstChild("LOL") then
				game.Workspace["LOL"].CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-3.5,0)
			end
		else
			if game:GetService("Workspace"):FindFirstChild("LOL") then
				game:GetService("Workspace"):FindFirstChild("LOL"):Destroy()
			end
		end
	end)
end)
